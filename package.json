{
  "name": "arcana-wallet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint": "run-s lint:all:*",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "lint:all:eslint": "npm run lint:eslint -- . --ext .vue,.ts --ignore-path .gitignore",
    "lint:all:markdownlint": "npm run lint:markdownlint -- \"**/*.md\" --ignore-path .gitignore",
    "lint:all:prettier": "npm run lint:prettier -- \"**/*.{ts,json,css,scss,vue,html,md}\" --ignore-path .gitignore",
    "lint:all:stylelint": "npm run lint:stylelint -- \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@arcana/auth": "https://testnet-dev.s3.ap-south-1.amazonaws.com/arcana-auth-0.0.9.tgz",
    "@ethereumjs/tx": "^3.4.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "core-js": "^3.8.3",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.11.1",
    "eth-crypto": "^2.1.0",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.1.3",
    "ethers": "^5.5.2",
    "floating-vue": "^2.0.0-beta.13",
    "penpal": "^6.2.2",
    "pinia": "^2.0.12",
    "sass": "^1.51.0",
    "stream-browserify": "^3.0.0",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-json-viewer": "^3.0.4",
    "vue-router": "^4.0.14",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "markdownlint-cli": "^0.31.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "~4.5.5",
    "vite": "^2.9.6",
    "vue-tsc": "^0.34.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
